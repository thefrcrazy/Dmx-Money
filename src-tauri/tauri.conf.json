{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "DmxMoney",
  "version": "0.7.0",
  "identifier": "com.dmxmoney.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/thefrcrazy/Dmx-Money/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDNENTFFRTVDNjFCRjIyQ0QKUldUTklyOWhYTzVSUFF2aE81TlA5bisveFM5THlHSlBOaFlpbWpTMmdqc3U3bDlpZG05UnFVbVgK"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15"
    },
    "fileAssociations": [
      {
        "ext": [
          "dmx"
        ],
        "name": "DMX Money Backup",
        "description": "DMX Money backup file",
        "role": "Editor",
        "mimeType": "application/x-dmxmoney-backup"
      }
    ]
  }
}